{
  "name": "anonymous-chat",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "copyfiles": "^1.0.0",
    "firebase": "^3.2.1",
    "material-design-lite": "^1.1.3",
    "moment": "^2.14.1",
    "react-mixin": "^2.0.2",
    "react-scripts": "0.2.1",
    "reactfire": "^1.0.0",
    "sw-precache": "^4.0.0"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-images && npm run copy-assets && npm run pre-cache",
    "copy-images": "copyfiles -f './src/images/**.*' './build/static/images'",
    "copy-assets": "copyfiles -f './manifest.json' './build/'",
    "pre-cache": "sw-precache --root=build --config=./sw-precache-config.json --verbose --no-handle-fetch"
  },
  "homepage": "https://gokulkrishh.github.io/anonymous-web",
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
